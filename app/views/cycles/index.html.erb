<h1>Cycle#index</h1>
<p>Find me in app/views/cycle/index.html.erb</p>

<ul>
  <% @cycles.each do |cycle| %>
    <li>
      <%= link_to cycle.title, cycle_path(cycle) %> : <%= cycle.description %> - <%= cycle.color %> - <%= cycle.general_state %> - <%= cycle.location %> - <%= cycle.price %></p>
    </li>
  <% end %>
</ul>

<p><%= link_to "New", new_cycle_path, class: "btn btn-primary" %></p>
